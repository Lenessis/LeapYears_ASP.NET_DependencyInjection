@page
@model LeapYears.Pages.History.IndexModel

@{
    ViewData["Title"] = "History";
}

<h1>History</h1>

@*<p>
    <a asp-page="Create">Create New</a>
</p>*@
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.History[0].date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.History[0].result)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.History[0].YearUser)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.History) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.result)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.YearUser.name) @Html.DisplayFor(modelItem => item.YearUser.lastname)
            </td>
            <td>
                @*<a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
            <a asp-page="./Details" asp-route-id="@item.Id">Details</a>|*@
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
